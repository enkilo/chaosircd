      printf "%20s: %s" "User executables" ""
      $as_echo "$as_me:$LINENO: result: $bindir" >&5 ; echo -e ${COLOR_DIR}$bindir${NC} >&6 ;;    "sbindir")
      eval sbindir="$sbindir" ; eval sbindir="$sbindir"
      printf "%20s: %s" "System admin executables" ""
      $as_echo "$as_me:$LINENO: result: $sbindir" >&5 ; echo -e ${COLOR_DIR}$sbindir${NC} >&6 ;;    "libexecdir")
