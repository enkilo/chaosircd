       ac_top_srcdir=.
    else
       ac_top_srcdir=`echo $ac_top_builddir | sed 's,/$,,'`
    fi ;;
  [\\/]* | ?:[\\/]* )  # Absolute path.
    ac_srcdir=$srcdir$ac_dir_suffix;
    ac_top_srcdir=$srcdir ;;
  *) # Relative path.
    ac_srcdir=$ac_top_builddir$srcdir$ac_dir_suffix
    ac_top_srcdir=$ac_top_builddir$srcdir ;;
esac

ac_thisname="$ac_dir_name";
ac_thisdir="$ac_dir_name/";

# Do not use `cd foo && pwd` to compute absolute paths, because
# the directories may not exist.


    cd $ac_dir

    # Check for guested configure; otherwise get Cygnus style configure.
    if test -f $ac_srcdir/configure.gnu; then
      ac_sub_configure="$SHELL '$ac_srcdir/configure.gnu'"
    elif test -f $ac_srcdir/configure; then
      ac_sub_configure="$SHELL '$ac_srcdir/configure'"
    elif test -f $ac_srcdir/autogen.sh; then
        ac_sub_configure="$SHELL '$ac_srcdir/autogen.sh'"
    elif test -f $ac_srcdir/configure.in; then
      ac_sub_configure=$ac_configure
    else
      { echo "$as_me:$LINENO: WARNING: no configuration information is in $ac_dir" >&5
echo "$as_me: WARNING: no configuration information is in $ac_dir" >&2;}
      ac_sub_configure=
    fi



    # The recursion is here.
    if test -n "$ac_sub_configure"; then
      # Make the cache file name correct relative to the subdirectory.
      case $cache_file in
      [\\/]* | ?:[\\/]* ) ac_sub_cache_file=$cache_file ;;
      *) # Relative path.
  ac_sub_cache_file=$ac_top_builddir$cache_file ;;
      esac

      { echo "$as_me:$LINENO: running $ac_sub_configure $ac_sub_configure_args --cache-file=$ac_sub_cache_file --srcdir=$ac_srcdir" >&5
echo "$as_me: running $ac_sub_configure $ac_sub_configure_args --cache-file=$ac_sub_cache_file --srcdir=$ac_srcdir" >&6;}
      # The eval makes quoting arguments work.
      eval $ac_sub_configure $ac_sub_configure_args \
     --cache-file=$ac_sub_cache_file --srcdir=$ac_srcdir ||
  { { echo "$as_me:$LINENO: error: $ac_sub_configure failed for $ac_dir" >&5
echo "$as_me: error: $ac_sub_configure failed for $ac_dir" >&2;}
   { (exit 1); exit 1; }; }
    fi
    cd $ac_popdir
  done
fi


# be verbose :)
# ---------------------------------------------------------------------------
echo
echo "$PACKAGE_NAME-$PACKAGE_VERSION has been configured with the following options:"
echo


# if no args given, display all substituted dirs
ac_summarize_dirs="prefix bindir datadir includedir"


# loop through substituted dirs and show info
for ac_summarize_dir in $ac_summarize_dirs
do
  case $ac_summarize_dir in     "prefix")
      eval prefix="$prefix" ; eval prefix="$prefix"
      printf "%20s: %s" "Prefix" ""
      echo "$as_me:$LINENO: result: $prefix" >&5 ; echo -e ${COLOR_DIR}$prefix${NC} >&6 ;;    "bindir")
      eval bindir="$bindir" ; eval bindir="$bindir"
      printf "%20s: %s" "User executables" ""
      echo "$as_me:$LINENO: result: $bindir" >&5 ; echo -e ${COLOR_DIR}$bindir${NC} >&6 ;;    "sbindir")
      eval sbindir="$sbindir" ; eval sbindir="$sbindir"
      printf "%20s: %s" "System admin executables" ""
      echo "$as_me:$LINENO: result: $sbindir" >&5 ; echo -e ${COLOR_DIR}$sbindir${NC} >&6 ;;    "libexecdir")
      eval libexecdir="$libexecdir" ; eval libexecdir="$libexecdir"
      printf "%20s: %s" "Program executables" ""
      echo "$as_me:$LINENO: result: $libexecdir" >&5 ; echo -e ${COLOR_DIR}$libexecdir${NC} >&6 ;;    "datadir")
      eval datadir="$datadir" ; eval datadir="$datadir"
      printf "%20s: %s" "Read-only data" ""
      echo "$as_me:$LINENO: result: $datadir" >&5 ; echo -e ${COLOR_DIR}$datadir${NC} >&6 ;;    "sysconfdir")
      eval sysconfdir="$sysconfdir" ; eval sysconfdir="$sysconfdir"
      printf "%20s: %s" "Configuration files" ""
      echo "$as_me:$LINENO: result: $sysconfdir" >&5 ; echo -e ${COLOR_DIR}$sysconfdir${NC} >&6 ;;    "sharedstatedir")
      eval sharedstatedir="$sharedstatedir" ; eval sharedstatedir="$sharedstatedir"
      printf "%20s: %s" "Portable data" ""
      echo "$as_me:$LINENO: result: $sharedstatedir" >&5 ; echo -e ${COLOR_DIR}$sharedstatedir${NC} >&6 ;;    "localstatedir")
      eval localstatedir="$localstatedir" ; eval localstatedir="$localstatedir"
      printf "%20s: %s" "Binary data" ""
      echo "$as_me:$LINENO: result: $localstatedir" >&5 ; echo -e ${COLOR_DIR}$localstatedir${NC} >&6 ;;    "libdir")
      eval libdir="$libdir" ; eval libdir="$libdir"
      printf "%20s: %s" "Libraries" ""
      echo "$as_me:$LINENO: result: $libdir" >&5 ; echo -e ${COLOR_DIR}$libdir${NC} >&6 ;;    "includedir")
      eval includedir="$includedir" ; eval includedir="$includedir"
      printf "%20s: %s" "C header files" ""
      echo "$as_me:$LINENO: result: $includedir" >&5 ; echo -e ${COLOR_DIR}$includedir${NC} >&6 ;;    "oldincludedir")
      eval oldincludedir="$oldincludedir" ; eval oldincludedir="$oldincludedir"
      printf "%20s: %s" "C header files for non-gcc" ""
      echo "$as_me:$LINENO: result: $oldincludedir" >&5 ; echo -e ${COLOR_DIR}$oldincludedir${NC} >&6 ;;    "infodir")
      eval infodir="$infodir" ; eval infodir="$infodir"
      printf "%20s: %s" "Info documentation" ""
      echo "$as_me:$LINENO: result: $infodir" >&5 ; echo -e ${COLOR_DIR}$infodir${NC} >&6 ;;    "mandir")
      eval mandir="$mandir" ; eval mandir="$mandir"
      printf "%20s: %s" "Manual pages" ""
      echo "$as_me:$LINENO: result: $mandir" >&5 ; echo -e ${COLOR_DIR}$mandir${NC} >&6 ;;
    *)
      # print an empty line
      echo
      ;;
  esac
done
echo


# if no args given, display all substituted vars
ac_summarize_vars="COLOR DEP DEBUG |
              CC CFLAGS LDFLAGS LIBS |
              host build"


# loop through substituted vars and show info
for ac_summarize_var in $ac_summarize_vars
do
  case $ac_summarize_var in
    "host")
      host=`echo $host`
      if test "$host"; then
        printf "%20s: %s" "Host system" ""
        echo "$as_me:$LINENO: result: $host" >&5 ; echo -e ${COLOR_INFO}$host${NC} >&6
      fi
      ;;

    "build")
      build=`echo $build`
      if test "$build"; then
        printf "%20s: %s" "Build system" ""
        echo "$as_me:$LINENO: result: $build" >&5 ; echo -e ${COLOR_INFO}$build${NC} >&6
      fi
      ;;

    "target")
      target=`echo $target`
      if test "$target"; then
        printf "%20s: %s" "Target system" ""
        echo "$as_me:$LINENO: result: $target" >&5 ; echo -e ${COLOR_DEFAULT}$target${NC} >&6

      fi
      ;;

    "CC")
      CC=`echo $CC`
      if test "$CC"; then
        printf "%20s: %s" "Compiler" ""
        echo "$as_me:$LINENO: result: $CC" >&5 ; echo -e ${COLOR_PROG}$CC${NC} >&6
      fi
      ;;

    "CXX")
      CXX=`echo $CXX`
      if test "$CXX"; then
        printf "%20s: %s" "C++ compiler" ""
        echo "$as_me:$LINENO: result: $CXX" >&5 ; echo -e ${COLOR_DEFAULT}$CXX${NC} >&6

      fi
      ;;

    "CFLAGS")
      CFLAGS=`echo $CFLAGS`
      if test "$CFLAGS"; then
