      yes,yes)
        { echo "$as_me:$LINENO: WARNING: On windows you will want to build either shared OR static libraries, not both" >&5
echo "$as_me: WARNING: On windows you will want to build either shared OR static libraries, not both" >&2;}
        A_ENABLE='no'
        ;;
    esac
    ;;
  *)
    if test "$PIE_ENABLE" = "yes"; then
      { echo "$as_me:$LINENO: WARNING: Your system is not supported for shared linking" >&5
echo "$as_me: WARNING: Your system is not supported for shared linking" >&2;}
      PIE_ENABLE="no"
    fi
    ;;
esac

